{% extends 'back.html.twig' %}

{% block title %}Partner{% endblock %}


{% block content %}
    <div style="margin-top: 4vw;margin-left: auto;margin-right: auto; max-width: 70%">
        <h1>Partenaire</h1>

        <table class="table">
            <tbody>
            <tr>
                <th>IdPart</th>
                <td>{{ partner.idPart }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ partner.nom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ partner.adresse }}</td>
            </tr>
            <tr>
                <th>Logo</th>
                <td><img src="{{ asset('imagepartenaire/')~ partner.logo ~''}}"  height=60% width="60%" height="auto" display="block"  border="0"></td>
            </tr>
            <tr>
                <th>Rib</th>
                <td>{{ partner.rib }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ partner.tel }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ partner.status }}</td>
            </tr>
            </tbody>
        </table>

        <a href="{{ path('partenaire_back_index') }}">back to list</a>

        <a href="{{ path('partenaire_back_edit', {'id': partner.idPart}) }}">edit</a>
        {{ include('partenaire/back_delete_form.html.twig') }}
    </div>

{% endblock %}
